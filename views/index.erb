<h2>Many items for sale...</h2>
<h3>Sign-up or Log-in to sell your stuff or contact a seller</h3>
<% @items.each do |item| %>

<ul>
  <li>
    <a href="/item_info/<%= item.id%>">
      <img id="index" src="<%= item.image_url%>" alt="">
      <%= item.name %>
      $<%= item.price %>
    </a>
  </li>
</ul>




<% end %>
